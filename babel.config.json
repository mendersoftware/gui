{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": "3",
        "targets": { "node": "current" },
        "useBuiltIns": "usage"
      }
    ],
    ["@babel/preset-react", { "runtime": "automatic" }]
  ],
  "plugins": ["@babel/plugin-syntax-dynamic-import", "@babel/plugin-transform-runtime"]
}
