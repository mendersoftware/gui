    "@mdi/js": "^5.6.55",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.9.0",
    "axios": "^0.20.0",
    "copy-to-clipboard": "^3.3.1",
    "generate-password": "^1.5.1",
    "jwt-decode": "^3.0.0",
    "lodash.differencewith": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "md5": "^2.3.0",
    "moment": "^2.29.0",
    "moment-duration-format": "^2.3.2",
    "pluralize": "^8.0.0",
    "react": "~16.13.1",
    "react-big-calendar": "^0.28.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "~16.13.1",
    "react-dropzone": "^11.2.0",
    "react-ga": "^3.1.2",
    "react-google-recaptcha": "^2.1.0",
    "react-idle-timer": "^4.3.6",
    "react-linkify": "^1.0.0-alpha",
    "react-motion": "^0.5.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-time": "^4.3.0",
    "react-tooltip": "^3.11.6",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "universal-cookie": "^4.0.4",
    "validator": "^13.1.17",
    "victory": "^35.0.9",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^10.0.0",
    "axios-mock-adapter": "^1.18.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "~26.3.0",
    "babel-loader": "^8.1.0",
    "child_process": "^1.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "core-js": "^3.6.5",
    "css-loader": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.10.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "file-loader": "^6.1.0",
    "gulp": "^4.0.2",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "~26.4.2",
    "jest-environment-jsdom-fifteen": "1.0.2",
    "jest-resolve": "~26.4.0",
    "jest-watch-typeahead": "0.6.1",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "lint-staged": "^10.4.0",
    "mini-css-extract-plugin": "^0.11.3",
    "postcss": "^8.0.7",
    "postcss-loader": "^4.0.2",
    "prettier": "^2.1.2",
    "react-test-renderer": "~16.13.1",
    "redux-mock-store": "^1.5.4",
    "resolve-url-loader": "^3.1.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "yarn": "^1.22.10"
  },
  "browserslist": [
    ">0.2%",
    "last 5 versions",
    "not dead",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mendersoftware/gui.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "sh ${MENDER_TESTING}/check_commits.sh"
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/themes/"
    ],
    "setupFilesAfterEnv": [
      "./tests/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js",
      "<rootDir>/src/**/*.{spec,test}.js"
    ],
    "testEnvironment": "jest-environment-jsdom-fifteen",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.(css|less)$": "<rootDir>/tests/cssTransform.js",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/assetsTransformer.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.js$",
      "^.+\\.module\\.(css|less|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "scripts": {
    "build": "webpack --mode production ./src/js/main.js",
    "disclaim": "yarn licenses generate-disclaimer > disclaimer.txt",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "test": "node test.js",
    "watch": "webpack --watch --mode development ./src/js/main.js"
  },
  "author": "Michael Clelland",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mendersoftware/gui/issues"
  },
  "homepage": "https://github.com/mendersoftware/gui#readme",
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
