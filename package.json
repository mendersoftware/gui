  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@emotion/jest": "^11.10.0",
    "@svgr/webpack": "^6.3.1",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "assert": "^2.0.0",
    "autoprefixer": "^10.4.8",
    "axios-mock-adapter": "^1.21.1",
    "babel-jest": "~28.1.3",
    "child_process": "^1.0.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.24.1",
    "css-loader": "^6.7.1",
    "esbuild-loader": "^2.19.0",
    "eslint": "^8.21.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-sonarjs": "^0.14.0",
    "eslint-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^28.1.3",
    "jest-junit": "^14.0.0",
    "jest-resolve": "~28.1.3",
    "jest-watch-typeahead": "2.0.0",
    "jsdom-worker": "^0.2.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "msw": "^0.46.0",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "redux-mock-store": "^1.5.4",
    "util": "^0.12.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "yarn": "^1.22.19"
  },
  "browserslist": [
    ">0.2%",
    "last 5 versions",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=14",
    "npm": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mendersoftware/gui.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "sh ${MENDER_TESTING}/check_commits.sh"
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/themes/"
    ],
    "coverageReporters": [
      "lcov",
      "cobertura",
      "text"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.js"
    ],
    "snapshotSerializers": [
      "@emotion/jest/serializer"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js",
      "<rootDir>/src/**/*.{spec,test}.js"
    ],
    "fakeTimers": {
      "enableGlobally": true
    },
    "transform": {
      "\\.[j|t]sx?$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/assetsTransformer.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!xterm-for-react)"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/tests/cssTransform.js"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "scripts": {
    "build": "webpack --mode production --entry ./src/js/main.js",
    "disclaim": "yarn licenses generate-disclaimer > disclaimer.txt",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "serve": "./entrypoint.sh && webpack serve  --mode development --entry ./src/js/main.js",
    "test": "node test.js",
    "watch": "webpack --watch --mode development --entry ./src/js/main.js"
  },
  "author": "Northern.tech AS",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://tracker.mender.io/projects/MEN"
  },
  "homepage": "https://mender.io/",
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
