{
  "name": "mender-gui",
  "version": "1.0.0",
  "description": "Graphical user interface for Mender.",
  "dependencies": {
    "@date-io/moment": "^1.3.13",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@material-ui/pickers": "^3.3.10",
    "@mdi/js": "^6.2.95",
    "@reduxjs/toolkit": "^1.6.2",
    "@stripe/react-stripe-js": "^1.6.0",
    "@stripe/stripe-js": "^1.20.2",
    "axios": "^0.22.0",
    "buffer": "^6.0.3",
    "copy-to-clipboard": "^3.3.1",
    "crypto-browserify": "^3.12.0",
    "generate-password": "^1.6.1",
    "jwt-decode": "^3.1.2",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "msgpack5": "^5.3.2",
    "pluralize": "^8.0.0",
    "process": "^0.11.10",
    "react": "~16.14.0",
    "react-big-calendar": "^0.36.1",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "~16.14.0",
    "react-dropzone": "^11.4.2",
    "react-ga": "^3.3.0",
    "react-google-recaptcha": "^2.1.0",
    "react-idle-timer": "^4.6.4",
    "react-linkify": "^1.0.0-alpha",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-time": "^4.3.0",
    "react-tooltip": "^3.11.6",
    "redux-devtools-extension": "^2.13.9",
    "stream-browserify": "^3.0.0",
    "universal-cookie": "^4.0.4",
    "validator": "^13.6.0",
    "victory": "^36.0.1",
    "xterm": "^4.13.0",
    "xterm-addon-fit": "^0.5.0",
    "xterm-addon-search": "^0.8.1",
    "xterm-addon-web-links": "^0.4.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/dom": "^8.9.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.3.0",
    "assert": "^2.0.0",
    "autoprefixer": "^10.3.7",
    "axios-mock-adapter": "^1.20.0",
    "babel-jest": "~27.2.0",
    "child_process": "^1.0.2",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.18.3",
    "css-loader": "^6.4.0",
    "esbuild-loader": "^2.16.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.4.0",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "jest-canvas-mock": "^2.3.1",
    "jest-junit": "^13.0.0",
    "jest-resolve": "~27.3.0",
    "jest-watch-typeahead": "1.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.1.0",
    "lint-staged": "^11.2.3",
    "mini-css-extract-plugin": "^2.4.2",
    "msw": "^0.35.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "postcss": "^8.3.9",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.2.1",
    "redux-mock-store": "^1.5.4",
    "util": "^0.12.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "yarn": "^1.22.15"
  },
  "browserslist": [
    ">0.2%",
    "last 5 versions",
    "not dead",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mendersoftware/gui.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "sh ${MENDER_TESTING}/check_commits.sh"
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/themes/"
    ],
    "coverageReporters": [
      "lcov",
      "cobertura",
      "text"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.js"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js",
      "<rootDir>/src/**/*.{spec,test}.js"
    ],
    "timers": "fake",
    "transform": {
      "^.+\\.[j|t]sx?$": "babel-jest",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/assetsTransformer.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!xterm-for-react)"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/tests/cssTransform.js"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "scripts": {
    "build": "webpack --mode production --entry ./src/js/main.js",
    "disclaim": "yarn licenses generate-disclaimer > disclaimer.txt",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "test": "node test.js",
    "watch": "webpack --watch --mode development --entry ./src/js/main.js"
  },
  "author": "Michael Clelland",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mendersoftware/gui/issues"
  },
  "homepage": "https://github.com/mendersoftware/gui#readme",
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
