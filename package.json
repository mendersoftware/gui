  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.3",
    "assert": "^2.0.0",
    "autoprefixer": "^10.2.4",
    "axios-mock-adapter": "^1.19.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "~26.6.3",
    "babel-loader": "^8.2.2",
    "child_process": "^1.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.8.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.19.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.8",
    "jest": "next",
    "jest-canvas-mock": "^2.3.0",
    "jest-junit": "^12.0.0",
    "jest-resolve": "~26.6.2",
    "jest-watch-typeahead": "0.6.1",
    "less": "^4.1.1",
    "less-loader": "^7.2.1",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.5",
    "msw": "^0.26.1",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "~16.14.0",
    "redux-mock-store": "^1.5.4",
    "url-loader": "^4.1.1",
    "util": "^0.12.3",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "yarn": "^1.22.10"
  },
  "browserslist": [
    ">0.2%",
    "last 5 versions",
    "not dead",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mendersoftware/gui.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "sh ${MENDER_TESTING}/check_commits.sh"
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/themes/"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.js"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js",
      "<rootDir>/src/**/*.{spec,test}.js"
    ],
    "timers": "modern",
    "transform": {
      "^.+\\.[j|t]sx?$": "babel-jest",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/assetsTransformer.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!xterm-for-react)"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/tests/cssTransform.js"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "scripts": {
    "build": "webpack --mode production --entry ./src/js/main.js",
    "disclaim": "yarn licenses generate-disclaimer > disclaimer.txt",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "test": "node test.js",
    "watch": "webpack --watch --mode development --entry ./src/js/main.js"
  },
  "author": "Michael Clelland",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mendersoftware/gui/issues"
  },
  "homepage": "https://github.com/mendersoftware/gui#readme",
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
